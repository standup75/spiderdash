<div class="field">
  <%= label_tag :name %>
  <div class="value"><%= @tile.name %></div>
</div>
<div class="field">
  <label>&nbsp;</label>
  <%= image_tag @tile.picture_url, :class => "level-element-picture" %>
</div>
<div class="field">
  <%= label_tag :size %>
  <div class="value"><%= @tile.size %></div>
</div>
<div class="field">
  <%= label_tag :set %>
  <div class="value"><%= @tile.set %></div>
</div>
<%= link_to "delete this tiles >>", tile_path, :method => :delete, :class => "margin-top action-link", :confirm => "Are you sure you want to delete this tiles?" %>
<%= link_to "edit this tiles >>", edit_tile_path, :class => "margin-top-small action-link" %>
<%= link_to "duplicate this tiles >>", duplicate_tile_path, :class => "margin-top-small action-link" %>
<%= link_to "back >>", tiles_path, :class => "margin-top-small action-link" %>
